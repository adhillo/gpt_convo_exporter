{
  "manifest_version": 2,
  "name": "ChatGPT Conversation Exporter",
  "version": "1.0",
  "description": "Export transcript from any ChatGPT conversation page.",
  "permissions": ["activeTab"],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "48": "icon48.png"
    }
  },
  "icons": {
    "48": "icon48.png"
  },
  "content_scripts": [
    {
      "matches": ["https://*/*", "http://*/*"],
      "js": ["content.js"]
    }
  ]
}
